{
    "name": "@filelib/client",
    "version": "0.0.1",
    "description": "Filelib.com API Client.",
    "type": "module",
    "main": "./dist/index.js",
    "files": [
        "./dist/*",
        "README.md"
    ],
    "types": "./dist/types.d.ts",
    "typings": "./dist/types.d.ts",
    "exports": {
        ".": "./dist/index.js",
        "./node": "./dist/node/index.js",
        "./browser": "./dist/browser/index.js",
        "./types": "./dist/types.d.ts"
    },
    "scripts": {
        "build": "npm run build:browser --prefix . && npm run build:node --prefix=.",
        "build:browser": "rm -rf ./dist && copyfiles -u 1 \"src/**/*.d.ts\" dist &&  tsc -p ./tsconfig-build.json  --skipLibCheck ",
        "build:node": "bun build src/node --outdir ./dist/  --target node",
        "pack": "npm run build || true && mkdir -p packs && rm -rf ./packs/* && npm pack --pack-destination=./packs",
        "lint": "eslint .",
        "test": "bun test"
    },
    "dependencies": {
        "file-type": "^21.0.0",
        "ini": "^5.0.0",
        "jose": "^5.9.2",
        "uuid": "^10.0.0"
    },
    "peerDependencies": {
        "@justinmusti/storage": "^0.0.9",
        "@justinmusti/utils": "^0.0.8"
    },
    "devDependencies": {
        "@eslint/js": "^9.10.0",
        "@types/bun": "^1.1.11",
        "@types/eslint__js": "^8.42.3",
        "@types/ini": "^4.1.1",
        "@types/node": "^22.17.1",
        "@types/uuid": "^10.0.0",
        "bun": "^1.1.31",
        "bun-types": "^1.1.31",
        "copyfiles": "^2.4.1",
        "eslint": "^9.10.0",
        "prettier": "3.3.3",
        "typescript": "^5.5.4",
        "typescript-eslint": "^8.4.0"
    },
    "keywords": [
        "Filelib.com",
        "API",
        "Client"
    ],
    "author": "Filelib",
    "license": "MIT"
}
