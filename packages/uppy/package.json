{
    "name": "@filelib/uppy",
    "version": "0.0.1",
    "description": "Filelib.com Uppy uploader plugin",
    "main": "./dist/FilelibUppy.js",
    "types": "./dist/FilelibUppy.d.ts",
    "typings": "./dist/FilelibUppy.d.ts",
    "files": [
        "./dist/*",
        "README.md"
    ],
    "scripts": {
        "build": "rm -rf ./dist && copyfiles -u 1 \"src/**/*.d.ts\" dist &&  tsc -p ./tsconfig.json  --skipLibCheck ",
        "pack": "npm run build || true && mkdir -p packs && rm -rf ./packs/* && npm pack --pack-destination=./packs",
        "lint": "eslint .",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": "Filelib",
    "license": "MIT",
    "peerDependencies": {
        "@uppy/core": "^4.4.2",
        "@uppy/utils": "^6.1.2"
    },
    "devDependencies": {
        "@types/node": "^22.5.2",
        "copyfiles": "^2.4.1",
        "prettier": "^3.4.2",
        "typescript": "^5.5.4"
    },
    "dependencies": {
        "@filelib/client": "file:/home/musti/repos/filelib-npm/packages/client/packs/filelib-client-0.0.1.tgz",
        "@uppy/status-bar": "^4.1.1"
    }
}
